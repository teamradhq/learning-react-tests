{
  "name": "foodies-talk",
  "scripts": {
    "build:dev": "webpack --progress --env development --env NODE_ENV=development",
    "dev": "nodemon  --delay 2 --watch ./src --exec npm run go -V --ext ts,js,tsx,jsx,html,css,scss",
    "serve:dev": "node --inspect=0.0.0.0:9229 dist/server.js",
    "go": "npm run build:dev && npm run serve:dev",
    "lint": "eslint ./src --config ./eslint.config.js --fix",
    "lint:test": "eslint ./test --config ./eslint.jest.config.js --fix",
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --config ./jest.config.js --watchAll",
    "test:coverage": "jest --config ./jest.config.js --coverage --coverageDirectory ./coverage"
  },
  "dependencies": {
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.14.2",
    "@babel/plugin-transform-typescript": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@reduxjs/toolkit": "^1.5.1",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@types/classnames": "^2.3.1",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.23",
    "@types/jsdom": "^16.2.10",
    "@types/node": "^15.0.3",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-jest": "^27.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "classnames": "^2.3.1",
    "copy-webpack-plugin": "^9.0.0",
    "core-js": "^3.12.1",
    "css-loader": "^5.2.6",
    "dotenv": "^9.0.2",
    "eslint": "^7.26.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.4",
    "jest-extended": "^0.11.5",
    "jsdom": "^16.6.0",
    "nodemon": "^2.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.2",
    "ts-loader": "^9.2.2",
    "typescript": "^4.2.4",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0"
  }
